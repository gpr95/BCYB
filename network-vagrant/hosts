[routers]
r1 ansible_password=vagrant ansible_ssh_host=10.0.10.2 ansible_python_interpreter=/usr/local/bin/python2.7
r2 ansible_password=vagrant ansible_ssh_host=10.0.20.2 ansible_python_interpreter=/usr/local/bin/python2.7

[client-zone0]
